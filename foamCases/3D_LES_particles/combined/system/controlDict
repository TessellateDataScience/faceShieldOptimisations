FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     foamRun;

solver          incompressibleFluid;

startFrom       latestTime;

stopAt          endTime;

endTime         7.5;

writeInterval   0.5;

adjustTimeStep  yes;

maxCo           10;

maxDeltaT       0.01;

deltaT          0.0001;

writeControl    adjustableRunTime;

purgeWrite      0;

writeFormat     ascii;

writePrecision  3;

writeCompression off;

timeFormat      general;

timePrecision   3;

runTimeModifiable yes;

functions
{

    #includeFunc  probes

    /*clouds
    {
        type            fvModel;
        #includeModel clouds(name=fvModel)
    }*/
}

