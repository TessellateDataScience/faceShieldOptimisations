FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     foamRun;

solver          incompressibleFluid;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         10.0;

//maxCo           2.0;

//maxDeltaT       0.01;

deltaT          0.0001;

adjustTimeStep  no;

writeControl    adjustableRunTime;

writeInterval   0.02;

purgeWrite      0;

writeFormat     ascii;

writePrecision  3;

writeCompression off;

timeFormat      general;

timePrecision   3;

runTimeModifiable yes;

functions
{
    /*clouds
    {
        type            fvModel;
        #includeModel clouds(name=fvModel)
