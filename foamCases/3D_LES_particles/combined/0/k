FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    person
    {
        type            fixedValue;
        value           uniform 0;
    }
    mouth
    {
        type            inletOutlet;
        inletValue      uniform 0.375;
        value           uniform 0.375;
    }
    fan
    {
        type            fixedValue;
        inletValue      uniform 0.375;
        value           uniform 0.375;
    }
    nostrils
    {
        type            inletOutlet;
        inletValue      uniform 0.375;
        value           uniform 0.375;
    }
    wall
    {
        type            fixedValue;
        value           uniform 0;
    }
    atmos
    {
        type            fixedValue;
        value           uniform 0.375;
    }
    oldInternalFaces
    {
        type            internal;
    }
    interface
    {
        type            zeroGradient;
    }
    interfaceShield
    {
        type            zeroGradient;
    }
}


