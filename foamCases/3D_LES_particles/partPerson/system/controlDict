maxCo           0.5;

maxDeltaT       0.1;

deltaT          0.000001;

adjustTimeStep  yes;

writeControl    adjustableRunTime;

writeInterval   1.0;

purgeWrite      0;

writeFormat     ascii;

writePrecision  3;

writeCompression off;

timeFormat      general;

timePrecision   3;

runTimeModifiable yes;

