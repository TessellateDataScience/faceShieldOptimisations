FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     pisoFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         10;

deltaT          0.000005;

writeControl    timeStep;

writeInterval   50;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

functions
{
    //#includeFunc probes

    probes
    {
        type                probes;
        functionObjectLibs  ("libsampling.so");
        enabled             true;
        writeControl        timeStep;
        writeInterval       1;

        fields (U);

        probeLocations
        (
          (0 0 -0.001)
        );
    }
}

