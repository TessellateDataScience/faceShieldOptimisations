FoamFile
{
    format      ascii;
    class       dictionary;
    object      topoSetDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

actions
(
    // Create set of good cells (no zero-volumed cells)
    {
        action  new;
        type    cellSet;
        name    c1;
        source  cellToCell;
        set     zeroVolumeCells;
    }
    {
        name    c1;
        type    cellSet;
        action  invert;
    }
);

