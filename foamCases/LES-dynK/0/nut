FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    empty
    {
        type            empty;
    }
    mouth
    {
        type            zeroGradient;
    }
    wall
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }
    atmos
    {
        type            zeroGradient;
    }
    baffle0
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }
    baffle1
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }
}


