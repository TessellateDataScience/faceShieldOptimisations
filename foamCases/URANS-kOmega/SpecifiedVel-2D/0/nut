FoamFile
{
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    Boundary
    {
        type            zeroGradient;
    }

    Head-1
    {
        type            nutkWallFunction;
        value           uniform 0;
    }

    Head-2
    {
        type            nutkWallFunction;
        value           uniform 0;
    }    

    Mouth
    {
        type            zeroGradient;
    }

    defaultFaces
    {
        type            empty;
    }
}

